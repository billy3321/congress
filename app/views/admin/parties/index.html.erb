<h1>Listing admin_parties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_parties.each do |admin_party| %>
      <tr>
        <td><%= admin_party.name %></td>
        <td><%= admin_party.image %></td>
        <td><%= link_to 'Show', [:admin, admin_party] %></td>
        <td><%= link_to 'Edit', edit_admin_party_path(admin_party) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_party], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Party', new_admin_party_path %>
