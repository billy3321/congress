<h1>Listing admin_entries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_entries.each do |admin_entry| %>
      <tr>
        <td><%= admin_entry.title %></td>
        <td><%= admin_entry.category_id %></td>
        <td><%= admin_entry.description %></td>
        <td><%= admin_entry.url %></td>
        <td><%= link_to 'Show', [:admin, admin_entry] %></td>
        <td><%= link_to 'Edit', edit_admin_entry_path(admin_entry) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_entry], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_admin_entry_path %>
